#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {
	
	float nota, promedio;
  int cant, cont = 0;
	
	printf("Cuantas notas tenes? \n");
	scanf("%f", cant);
	
	do {
	
	printf("dame la nota \n");
	scanf("%f", &nota);
	if(nota <= 1 && nota >= 10){
	promedio = promedio + nota;

		cont++;
		
		} else
		printf("La nota tiene que ser entre 1 y 10");
		
		}while(cont >= 0 && cont <= cant);	
		
		promedio = promedio / 4;
		
		if(promedio >= 6 && promedio > 0){
		
		printf("aprobaste con %f", promedio);
		
	
		} else if(promedio <= 5 && promedio > 0){
	
		printf("desaprobaste con %f", promedio);
	}
	
	return 0;
}
